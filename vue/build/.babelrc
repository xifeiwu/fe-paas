{
  "presets": [["es2015", { "loose": true, "comment": true }]],
  "plugins": ["transform-vue-jsx", "syntax-dynamic-import"],
  "env": {
    "utils": {
      "plugins": [
        ["module-resolver", {
          "root": ["element-ui"],
          "alias": {
            "element-ui/src": "element-ui/lib"
          }
        }]
      ]
    }
  }
}